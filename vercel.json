{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "cleanDistDir": true
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }