{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "cleanDistDir": true
        }
      }
    ],
    "routes": [
    {
      "src": "/assets/.*",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
  }