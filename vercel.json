{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "cleanDistDir": true,
          "includeFiles": ["**/*.css"]
          
        }
      }
    ],
    "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
         "Content-Type": {
            ".css" : "text/css",
            ".js" : "application/javascript"
            } 
        }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
  }